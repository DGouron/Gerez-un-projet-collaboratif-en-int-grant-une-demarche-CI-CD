<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    
    <!-- Suppress test dependencies vulnerabilities -->
    <suppress>
        <notes><![CDATA[
        Test dependencies should not be included in production builds
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/.*@.*test.*$</packageUrl>
    </suppress>
    
    <!-- Suppress Spring Boot development tools -->
    <suppress>
        <notes><![CDATA[
        Spring Boot DevTools is only used in development
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.springframework\.boot/spring-boot-devtools@.*$</packageUrl>
    </suppress>
    
    <!-- Example: Suppress known false positive -->
    <!-- 
    <suppress>
        <notes><![CDATA[
        This vulnerability does not affect our application because...
        ]]></notes>
        <cve>CVE-XXXX-XXXX</cve>
    </suppress>
    -->
    
</suppressions> 